<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhj.mapper.SysMembersMapper">

    <select id="getByMemberStuid" resultType="com.zhj.entity.SysMembers">
        select *
        from sys_members
        where
        member_stuid = #{memberStuid,jdbcType=NUMERIC}
    </select>

</mapper>
